	doi = {10.1093/mnras/stz1350},
	eprint = {1810.06895},
	journal = {\mnras},
	keywords = {methods: statistical, techniques: high angular resolution, techniques: image processing, planets and satellites: detection, Astrophysics - Instrumentation and Methods for Astrophysics},
	month = aug,
	number = {2},
	pages = {2262-2277},
	primaryclass = {astro-ph.IM},
